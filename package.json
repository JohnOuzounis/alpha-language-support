{
  "name": "alpha-language-support",
  "displayName": "Alpha Language Support",
  "description": "Alpha Language Support",
  "version": "5.2.14",
  "publisher": "JohnOuz",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnOuzounis/alpha-language-support"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:al"
  ],
  "main": "./scripts/extension.js",
  "contributes": {
    "capabilities": {
      "completionProvider": {
        "resolveProvider": "true",
        "triggerCharacters": [
          "."
        ]
      },
      "hoverProvider": true
    },
    "commands": [
      {
        "command": "extension.parseGrammar",
        "title": "Alpha Parse Grammar"
      },
      {
        "command": "extension.compileAndRunVM",
        "title": "Alpha Compile and Run VM"
      }
    ],
    "languages": [
      {
        "id": "alpha",
        "aliases": [
          "alpha",
          "alpha"
        ],
        "extensions": [
          ".al"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "alpha",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "grammars": [
      {
        "language": "alpha",
        "scopeName": "source.al",
        "path": "./syntaxes/alpha.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "files": [
    "bin/alpha.exe",
    "bin/alphac.exe",
    "bin/parser.exe"
  ],
  "devDependencies": {
    "typescript": "^5.1.3"
  }
}